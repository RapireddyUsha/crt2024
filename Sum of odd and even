#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int a[n],i,s=0,s1=0;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
        }
    for(i=0;i<n;i++){
        if(a[i]%2==0){
           s=s+a[i];

            }
        else{
            s1=s1+a[i];
            }
        }
    printf("%d\n%d",s1,s);   
    return 0;
}
