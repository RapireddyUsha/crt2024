#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
int m,n;
    scanf("%d%d",&m,&n);
    int i,j;
    int a[m][n],max=0,res[m];
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
          scanf("%d",&a[i][j]);
    }
    for(i=0;i<m;i++)
    {  
        for(j=0;j<n;j++)
            if(a[i][j]>max)
                max=a[i][j];
        res[i]=max;
        max=0;
    }
     for(i=0;i<m;i++){
     printf("%d ",res[i]);
        printf("\n");
     }   
    return 0;
}
