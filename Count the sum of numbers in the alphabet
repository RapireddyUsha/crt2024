import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.next();
        int sum=0,res=0;
        for(int i=0;i<str.length();i++){
            char ch=str.charAt(i);
            if(Character.isDigit(ch)){
                res=ch-48;
                sum=sum+res;
            }
        }
            System.out.print(sum);
    }
}
