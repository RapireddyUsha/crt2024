import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str1=s.next();
        char str[]=str1.toCharArray();
        Arrays.sort(str);
        for(int i=0;i<str.length;i++){
            System.out.print(str[i]);
        }
            
    }
}
