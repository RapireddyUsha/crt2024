#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int m,n;
    scanf("%d%d",&m,&n);
    int i,j,sum=0;
    int a[m][n];
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
          scanf("%d",&a[i][j]);
    }
    for(i=0;i<n;i++)
    {  
        sum=0;
        for(j=0;j<m;j++)
        {
          sum=sum+a[j][i];                    
        }
    
        printf("%d\n",sum);
    
    }    
    return 0;
}
