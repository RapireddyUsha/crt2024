#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int age,weight;
    float hmg;
    scanf("%d%d%f",&age,&weight,&hmg);
    if((age>=18)&&(weight>=45)&&(hmg>=5.5)){
        printf("He or She can donate blood");
    }
    else{
        printf("He or She unable to donate blood");
    }  
    return 0;
}
